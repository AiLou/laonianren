<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wu.mapper.SortLibMapper">
    <select id="getAll" resultType="SortLib">
        select * from sortlib;
    </select>
    <select id="getById" resultType="SortLib">
        select * from sortlib where id = #{id};
    </select>
</mapper>
